{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ],
  "buildCommand": "ROLLUP_NO_NATIVE=true npm run build",
  "installCommand": "npm install --no-optional --legacy-peer-deps",
  "env": {
    "ROLLUP_NO_NATIVE": "true"
  }
}